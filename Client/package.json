{
  "name": "alumni-client",
  "version": "0.0.0",
  "engines": {
    "node": ">=12.18.3"
  },
  "scripts": {
    "build:tailwind": "tailwindcss build src/assets/css/tailwind/tailwind.css -o src/assets/css/tailwind/tailwind.output.css",
    "build:tailwind:prod": "tailwindcss build -c tailwind.config.prod.js src/assets/css/tailwind/tailwind.css -o src/assets/css/tailwind/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind:prod",
    "ng": "ng",
    "predev:prod": "npm run prebuild",
    "dev:prod": "ng serve --configuration production",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --base-href \"/Alumni/\" --prod --aot --output-hashing=all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.4",
    "@angular/cdk": "~12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "^12.2.4",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@angular/service-worker": "~12.2.4",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@rxweb/reactive-form-validators": "^2.1.3",
    "@swimlane/ngx-charts": "^18.0.1",
    "angular-animations": "^0.11.0",
    "aos": "^2.3.4",
    "chart.js": "^3.3.2",
    "clone": "^2.1.2",
    "exceljs": "^1.12.0",
    "file-saver": "^2.0.5",
    "ng-recaptcha": "^8.0.0",
    "ngx-abbreviate-number": "0.0.2",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-pagination": "^5.1.1",
    "ngx-ui-loader": "^11.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.1.0",
    "quill": "^1.3.7",
    "rfdc": "^1.3.0",
    "rxjs": "~6.6.0",
    "tailwindcss": "^2.1.2",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@types/aos": "^3.0.4",
    "@types/clone": "^2.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/rfdc": "^1.2.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
